# Пример переменных для production (скопируйте в .env)

# База данных
DB_USER=postgres
DB_PASSWORD=ЗАМЕНИТЕ_НА_НАДЁЖНЫЙ_ПАРОЛЬ
DB_NAME=poker_club

# Секреты (сгенерируйте случайные строки: openssl rand -hex 32)
SESSION_SECRET=ваша_случайная_строка_64_символа
JWT_SECRET=другая_случайная_строка_64_символа
JWT_EXPIRES_IN=7d

# URL приложения (замените на ваш домен)
# APP_URL — адрес API (backend)
# FRONTEND_URL — адрес веб-приложения (frontend)
APP_URL=https://api.vash-site.ru
FRONTEND_URL=https://vash-site.ru
